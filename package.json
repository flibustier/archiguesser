{
  "name": "archiguesser",
  "version": "2.4.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "test:unit": "node --test",
    "test:e2e": "playwright test",
    "test:e2e:linux": "docker run -it --rm --ipc=host --volume=./:/mnt --volume=/mnt/node_modules  mcr.microsoft.com/playwright:v1.44.1-jammy /bin/sh -c 'cd /mnt && yarn install && yarn playwright test --update-snapshots'",
    "build": "vue-tsc --noEmit & vite build",
    "preview": "vite preview --port 4173",
    "deploy": "./scripts/deploy.sh",
    "convert": "./scripts/convert.sh",
    "download": "node ./scripts/download/cli.js"
  },
  "dependencies": {
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@babel/types": "7.26.0",
    "@playwright/test": "1.48.1",
    "@types/node": "20.16.12",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.3",
    "@vue/tsconfig": "0.5.1",
    "axios": "1.7.7",
    "eslint": "9.13.0",
    "eslint-plugin-vue": "9.29.1",
    "lightningcss": "1.27.0",
    "prettier": "3.3.3",
    "rollup-plugin-visualizer": "5.12.0",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vue-tsc": "2.1.6"
  }
}
