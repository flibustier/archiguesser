{
  "name": "archiguesser",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "deploy": "./scripts/deploy.sh",
    "convert:jpg": "./scripts/convert-jpg.sh",
    "convert:webp": "./scripts/convert-webp.sh",
    "check": "node ./scripts/suggestions-checker.js",
    "scrape": "node ./scripts/scrapper.js",
    "stats": "node ./scripts/extract-stats.js",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "3.2.40"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@types/node": "18.8.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/tsconfig": "0.1.3",
    "axios": "1.0.0",
    "eslint": "8.24.0",
    "eslint-plugin-vue": "9.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rollup-plugin-visualizer": "5.8.2",
    "typescript": "4.8.4",
    "vite": "3.1.4",
    "vue-tsc": "0.40.13"
  }
