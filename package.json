{
  "name": "archiguesser",
  "version": "2.4.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "test:unit": "node --test",
    "test:e2e": "playwright test",
    "test:e2e:linux": "docker run -it --rm --ipc=host --volume=./:/mnt --volume=/mnt/node_modules  mcr.microsoft.com/playwright:v1.44.1-jammy /bin/sh -c 'cd /mnt && yarn install && yarn playwright test --update-snapshots'",
    "build": "vue-tsc --noEmit & vite build",
    "preview": "vite preview --port 4173",
    "deploy": "./scripts/deploy.sh",
    "convert": "./scripts/convert.sh",
    "download": "node ./scripts/download/cli.js"
  },
  "dependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@babel/types": "7.26.0",
    "@playwright/test": "1.49.0",
    "@types/node": "22.10.1",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.4",
    "@vue/tsconfig": "0.7.0",
    "axios": "1.7.8",
    "eslint": "9.16.0",
    "eslint-plugin-vue": "9.31.0",
    "lightningcss": "1.28.2",
    "prettier": "3.4.1",
    "rollup-plugin-visualizer": "5.12.0",
    "typescript": "5.7.2",
    "vite": "6.0.2",
    "vue-tsc": "2.1.10"
  }
}
