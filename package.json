{
  "name": "archiguesser",
  "version": "2.4.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "test:unit": "node --test",
    "test:e2e": "playwright test",
    "test:e2e:linux": "docker run -it --rm --ipc=host --volume=./:/mnt --volume=/mnt/node_modules  mcr.microsoft.com/playwright:v1.44.1-jammy /bin/sh -c 'cd /mnt && npm install && npm run playwright test --update-snapshots'",
    "build": "vue-tsc --noEmit & vite build",
    "preview": "vite preview --port 4173",
    "deploy": "./scripts/deploy.sh",
    "convert": "./scripts/convert.sh",
    "convert:all": "for dir in $(ls -1 sources/ | grep -E '^[0-9]+$' | sort -n); do echo 'Processing $dir...'; deno task convert $dir; done",
    "download": "node ./scripts/download/cli.js"
  },
  "dependencies": {
    "vue": "3.5.27"
  },
  "devDependencies": {
    "@babel/types": "7.28.6",
    "@playwright/test": "1.58.1",
    "@types/node": "24.10.11",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "axios": "1.13.4",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.7.0",
    "lightningcss": "1.31.1",
    "prettier": "3.8.1",
    "rollup-plugin-visualizer": "6.0.5",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vue-tsc": "3.2.4"
  }
}
